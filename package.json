{
  "name": "digistreets-backend",
  "version": "1.0.0",
  "description": "DigiStreets VCC Platform - Backend API",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "npm run swagger && tsc && npm run copy-swagger",
    "copy-swagger": "mkdir -p dist/src/swagger && cp src/swagger/swagger.json dist/src/swagger/swagger.json || true",
    "start": "node dist/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test:auth": "node scripts/test-auth.js",
    "test:rate-limit": "node scripts/test-rate-limiting.js",
    "test:user": "node scripts/test-user-routes.js",
    "test:card": "node scripts/test-card-routes.js",
    "add-test-balance": "ts-node src/scripts/add-test-balance.ts",
    "test:schedulers": "ts-node scripts/test-schedulers.ts",
    "test:crypto-jobs": "ts-node scripts/test-crypto-only.ts",
    "swagger": "tsoa spec-and-routes",
    "swagger:spec": "tsoa spec",
    "swagger:routes": "tsoa routes",
    "test:crypto-processors": "ts-node scripts/test-crypto-processors.ts",
    "test:session-processors": "ts-node scripts/test-session-processors.ts",
    "test:crypto-full": "ts-node scripts/test-schedulers.ts --crypto-only",
    "check-test-cards": "ts-node scripts/check-test-cards.ts",
    "test:email": "ts-node scripts/test-email.ts"
  },
  "dependencies": {
    "@bull-board/api": "^6.11.1",
    "@bull-board/express": "^6.11.1",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/node": "^9.40.0",
    "@sentry/profiling-node": "^9.40.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/json2csv": "^5.0.7",
    "@types/pg": "^8.15.4",
    "@types/puppeteer": "^5.4.7",
    "@types/validator": "^13.15.2",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "bullmq": "^5.54.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "fastify": "^5.4.0",
    "google-auth-library": "^10.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.0",
    "pg": "^8.16.2",
    "puppeteer": "^24.11.1",
    "redis": "^5.5.6",
    "resend": "^4.6.0",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsoa": "^6.6.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "xss": "^1.0.15",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  }
}
